extends main

block meta
    title #{ content["title"] }
    meta(name='description', content=content["description"])

block content
    section
        h2 #{ article["title"] }
        p !{ article["abstract"] }
        time #{ article["date"] }

    section
        h2 #{ project["title"] }
        p !{ project["abstract"] }
        time #{ article["date"] }

    section
        h2 #{ about["title"] }
        p !{ about["abstract"] }
        time #{ about["date"] }